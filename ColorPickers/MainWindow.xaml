<Window x:Class="ColorPickers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ColorPickers"
        xmlns:ColorPicker="clr-namespace:ColorLibrary;assembly=ColorLibrary"
        mc:Ignorable="d" Background="#272727"
        Title="FineShare" Height="450" Width="800">
    <Grid>
        <ColorPicker:Pickers x:Name="picker" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="3,0,0,0"  ChanageColor="Pickers_ChanageColor">
        </ColorPicker:Pickers>
    </Grid>
</Window>
